<h1>Welcome to PriceMarx</h1>
<h2>X Marks the Spot - For Fun!</h2>
<div class="container">
  <div class="row">
    <div class="col-md-8">
  <% @tags.each do | tag | %>
  <% link_to tag.name, tag_path(tag) %>
  <% @pricemarks.tagged_with(tag).each do | pricemark | %>
  <% link_to pricemark.title, pricemark.url %>
  <% end %>
  <% end %>
